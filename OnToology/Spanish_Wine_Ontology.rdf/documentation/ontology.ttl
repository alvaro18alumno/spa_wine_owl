@prefix : <http://www.semanticweb.org/alvarobarba/ontologies/2023/3/spanish-wine-ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/alvarobarba/ontologies/2023/3/spanish-wine-ontology#> .

<http://www.semanticweb.org/alvarobarba/ontologies/2023/3/spanish-wine-ontology> rdf:type owl:Ontology ;
                                                                                  <http://purl.org/dc/terms/license> "http://purl.org/NET/rdflicense/cc-by4.0" ;
                                                                                  rdfs:comment "Ontology for Spanish Wine made by Álvaro Barba Sandoval. Dataset author: fedesoriano." ;
                                                                                  rdfs:label "Spanish Wine Quality" .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/description
<http://purl.org/dc/elements/1.1/description> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
<http://purl.org/dc/elements/1.1/title> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/license
<http://purl.org/dc/terms/license> rdf:type owl:AnnotationProperty .


###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
<http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2000/01/rdf-schema#sameAs
rdfs:sameAs rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2002/07/owl#author
owl:author rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#gYear
xsd:gYear rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/alvarobarba/ontologies/2023/3/spanish-wine-ontology#fromCountry
:fromCountry rdf:type owl:ObjectProperty ;
             owl:inverseOf :hasRegions ;
             rdfs:domain :Region ;
             rdfs:range :Country ;
             rdfs:comment "Region is from a Country" .


###  http://www.semanticweb.org/alvarobarba/ontologies/2023/3/spanish-wine-ontology#fromRegion
:fromRegion rdf:type owl:ObjectProperty ;
            owl:inverseOf :hasWineries ;
            rdfs:domain :Winery ;
            rdfs:range :Region ;
            rdfs:comment "Winery is from a Region" .


###  http://www.semanticweb.org/alvarobarba/ontologies/2023/3/spanish-wine-ontology#hasRegions
:hasRegions rdf:type owl:ObjectProperty ;
            rdfs:domain :Country ;
            rdfs:range :Region ;
            rdfs:comment "Country has regions" .


###  http://www.semanticweb.org/alvarobarba/ontologies/2023/3/spanish-wine-ontology#hasWineries
:hasWineries rdf:type owl:ObjectProperty ;
             rdfs:domain :Region ;
             rdfs:range :Winery ;
             rdfs:comment "Region has wineries" .


###  http://www.semanticweb.org/alvarobarba/ontologies/2023/3/spanish-wine-ontology#isGrapeType
:isGrapeType rdf:type owl:ObjectProperty ;
             rdfs:domain :Wine ;
             rdfs:range :GrapeType ;
             rdfs:comment "Wine variety. The type of grape used to produce the wine. Each grape variety has unique characteristics that affect the flavor, aroma, and texture of the resulting wine." .


###  http://www.semanticweb.org/alvarobarba/ontologies/2023/3/spanish-wine-ontology#producedBy
:producedBy rdf:type owl:ObjectProperty ;
            owl:inverseOf :produces ;
            rdfs:domain :Wine ;
            rdfs:range :Winery ;
            rdfs:comment "Wine is made by a Winery" .


###  http://www.semanticweb.org/alvarobarba/ontologies/2023/3/spanish-wine-ontology#produces
:produces rdf:type owl:ObjectProperty ;
          rdfs:domain :Winery ;
          rdfs:range :Wine ;
          rdfs:comment "Winery makes wine" .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/alvarobarba/ontologies/2023/3/spanish-wine-ontology#harvestYear
:harvestYear rdf:type owl:DatatypeProperty ;
             rdfs:domain :Wine ;
             rdfs:range xsd:gYear ;
             rdfs:comment "Year in which the grapes were harvested" .


###  http://www.semanticweb.org/alvarobarba/ontologies/2023/3/spanish-wine-ontology#hasAcidity
:hasAcidity rdf:type owl:DatatypeProperty ;
            rdfs:domain :GrapeType ;
            rdfs:range [ rdf:type rdfs:Datatype ;
                         owl:onDatatype xsd:integer ;
                         owl:withRestrictions ( [ xsd:minInclusive 1
                                                ]
                                                [ xsd:maxInclusive 5
                                                ]
                                              )
                       ] ;
            rdfs:comment "Acidity score, defined as wine's “pucker” or tartness; it's what makes a wine refreshing and your tongue salivate and want another sip [from 1-5]" .


###  http://www.semanticweb.org/alvarobarba/ontologies/2023/3/spanish-wine-ontology#hasBody
:hasBody rdf:type owl:DatatypeProperty ;
         rdfs:domain :GrapeType ;
         rdfs:range [ rdf:type rdfs:Datatype ;
                      owl:onDatatype xsd:integer ;
                      owl:withRestrictions ( [ xsd:minInclusive 1
                                             ]
                                             [ xsd:maxInclusive 5
                                             ]
                                           )
                    ] ;
         rdfs:comment "Body score, defined as the richness and weight of the wine in your mouth [from 1-5]" .


###  http://www.semanticweb.org/alvarobarba/ontologies/2023/3/spanish-wine-ontology#hasName
:hasName rdf:type owl:DatatypeProperty ;
         rdfs:domain :Wine ;
         rdfs:range xsd:string ;
         rdfs:comment "Has a especific Name." .


###  http://www.semanticweb.org/alvarobarba/ontologies/2023/3/spanish-wine-ontology#hasNumReviews
:hasNumReviews rdf:type owl:DatatypeProperty ;
               rdfs:domain :Wine ;
               rdfs:range xsd:unsignedInt ;
               rdfs:comment "Number of users that reviewed the wine" .


###  http://www.semanticweb.org/alvarobarba/ontologies/2023/3/spanish-wine-ontology#hasPrice
:hasPrice rdf:type owl:DatatypeProperty ;
          rdfs:domain :Wine ;
          rdfs:range xsd:decimal ;
          rdfs:comment "Wine price in euros (€)." .


###  http://www.semanticweb.org/alvarobarba/ontologies/2023/3/spanish-wine-ontology#hasRating
:hasRating rdf:type owl:DatatypeProperty ;
           rdfs:domain :Wine ;
           rdfs:range [ rdf:type rdfs:Datatype ;
                        owl:onDatatype xsd:decimal ;
                        owl:withRestrictions ( [ xsd:minInclusive 1
                                               ]
                                               [ xsd:maxInclusive 5
                                               ]
                                             )
                      ] ;
           rdfs:comment "Average rating given to the wine by the users [from 1-5]" .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/alvarobarba/ontologies/2023/3/spanish-wine-ontology#Cava
:Cava rdf:type owl:Class ;
      owl:equivalentClass [ rdf:type owl:Restriction ;
                            owl:onProperty :isGrapeType ;
                            owl:hasValue :Cava
                          ] ;
      rdfs:subClassOf :Wine .


###  http://www.semanticweb.org/alvarobarba/ontologies/2023/3/spanish-wine-ontology#Country
:Country rdf:type owl:Class ;
         rdfs:subClassOf [ rdf:type owl:Restriction ;
                           owl:onProperty :hasRegions ;
                           owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                           owl:onClass :Winery
                         ] ;
         rdfs:comment "Country (in this ontology only Spain is used)." .


###  http://www.semanticweb.org/alvarobarba/ontologies/2023/3/spanish-wine-ontology#GrapeType
:GrapeType rdf:type owl:Class ;
           rdfs:comment "Wine variety. The type of grape used to produce the wine. Each grape variety has unique characteristics that affect the flavor, aroma, and texture of the resulting wine." .


###  http://www.semanticweb.org/alvarobarba/ontologies/2023/3/spanish-wine-ontology#HighPrice
:HighPrice rdf:type owl:Class ;
           rdfs:subClassOf :Price ;
           rdfs:comment "Wine price is more than 100 euros." .


###  http://www.semanticweb.org/alvarobarba/ontologies/2023/3/spanish-wine-ontology#HighRating
:HighRating rdf:type owl:Class ;
            rdfs:subClassOf :Rating ;
            rdfs:comment "More than 1000 num_reviews." .


###  http://www.semanticweb.org/alvarobarba/ontologies/2023/3/spanish-wine-ontology#LowPrice
:LowPrice rdf:type owl:Class ;
          rdfs:subClassOf :Price ;
          rdfs:comment "Wine price is less than 30 euros." .


###  http://www.semanticweb.org/alvarobarba/ontologies/2023/3/spanish-wine-ontology#LowRating
:LowRating rdf:type owl:Class ;
           rdfs:subClassOf :Rating ;
           rdfs:comment "Less than 100 num_reviews." .


###  http://www.semanticweb.org/alvarobarba/ontologies/2023/3/spanish-wine-ontology#MediumPrice
:MediumPrice rdf:type owl:Class ;
             rdfs:subClassOf :Price ;
             rdfs:comment "Wine price is more than 30, but less than 100 euros." .


###  http://www.semanticweb.org/alvarobarba/ontologies/2023/3/spanish-wine-ontology#MediumRating
:MediumRating rdf:type owl:Class ;
              rdfs:subClassOf :Rating ;
              rdfs:comment "More than 100, but less than 1000 num_reviews." .


###  http://www.semanticweb.org/alvarobarba/ontologies/2023/3/spanish-wine-ontology#Price
:Price rdf:type owl:Class ;
       owl:equivalentClass [ rdf:type owl:Class ;
                             owl:unionOf ( :HighPrice
                                           :LowPrice
                                           :MediumPrice
                                         )
                           ] ;
       rdfs:subClassOf :Quality ;
       rdfs:comment "Classification of wines according to their price." .


###  http://www.semanticweb.org/alvarobarba/ontologies/2023/3/spanish-wine-ontology#Quality
:Quality rdf:type owl:Class ;
         rdfs:comment "The subclases are ValuePartition that describes only three certain values." .


###  http://www.semanticweb.org/alvarobarba/ontologies/2023/3/spanish-wine-ontology#Rating
:Rating rdf:type owl:Class ;
        owl:equivalentClass [ rdf:type owl:Class ;
                              owl:unionOf ( :HighRating
                                            :LowRating
                                            :MediumRating
                                          )
                            ] ;
        rdfs:subClassOf :Quality ;
        rdfs:comment "Rating depending on the number of reviews a wine has." .


###  http://www.semanticweb.org/alvarobarba/ontologies/2023/3/spanish-wine-ontology#Red_Wine
:Red_Wine rdf:type owl:Class ;
          owl:equivalentClass [ rdf:type owl:Restriction ;
                                owl:onProperty :isGrapeType ;
                                owl:hasValue :Toro_Red
                              ] ;
          rdfs:subClassOf :Wine ;
          rdfs:comment "The red wine is macerated with the skins on the grapes for a longer period of time." .


###  http://www.semanticweb.org/alvarobarba/ontologies/2023/3/spanish-wine-ontology#Region
:Region rdf:type owl:Class ;
        rdfs:subClassOf [ rdf:type owl:Restriction ;
                          owl:onProperty :hasWineries ;
                          owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onClass :Winery
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :fromCountry ;
                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onClass :Country
                        ] ;
        rdfs:comment "Region of the winery." .


###  http://www.semanticweb.org/alvarobarba/ontologies/2023/3/spanish-wine-ontology#Rose_Wine
:Rose_Wine rdf:type owl:Class ;
           rdfs:subClassOf :Wine ;
           rdfs:comment "The rosé wine is macerated with the skins on the grapes for a short period of time." .


###  http://www.semanticweb.org/alvarobarba/ontologies/2023/3/spanish-wine-ontology#SparklingWine
:SparklingWine rdf:type owl:Class ;
               owl:equivalentClass [ rdf:type owl:Restriction ;
                                     owl:onProperty :isGrapeType ;
                                     owl:hasValue :Sparkling
                                   ] ;
               rdfs:subClassOf :Wine ;
               rdfs:comment "Sparkling wine." .


###  http://www.semanticweb.org/alvarobarba/ontologies/2023/3/spanish-wine-ontology#White_Wine
:White_Wine rdf:type owl:Class ;
            rdfs:subClassOf :Wine ;
            rdfs:comment "The white wine is made using grapes without the skins and no seeds." .


###  http://www.semanticweb.org/alvarobarba/ontologies/2023/3/spanish-wine-ontology#Wine
:Wine rdf:type owl:Class ;
      rdfs:subClassOf [ rdf:type owl:Restriction ;
                        owl:onProperty :isGrapeType ;
                        owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onClass :GrapeType
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :producedBy ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onClass :Winery
                      ] ;
      rdfs:comment "Oldest and most popular alcoholic beverages around the world, and is produced in many different wine regions using unique grape varieties and fermentation methods. The wine can be white, rosé, or red in color, and can vary in flavor, body, and aroma depending on the region, the type of grape, and the production technique used. In addition to being a popular drink to accompany meals, wine also plays an important role in culture and history, and is valued for its flavor, complexity, and ability to improve with age." .


###  http://www.semanticweb.org/alvarobarba/ontologies/2023/3/spanish-wine-ontology#Winery
:Winery rdf:type owl:Class ;
        rdfs:subClassOf [ rdf:type owl:Restriction ;
                          owl:onProperty :fromRegion ;
                          owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onClass :Region
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :produces ;
                          owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onClass :Wine
                        ] ;
        rdfs:comment "Wine factory. The place where the wine is made and stored, from the harvest of the grapes to its bottling and distribution. Wineries can range in size from small family wineries to large industrial facilities, and may be located in different wine regions around the world." .


#################################################################
#    Individuals
#################################################################

###  http://www.semanticweb.org/alvarobarba/ontologies/2023/3/spanish-wine-ontology#A_Coroa
:A_Coroa rdf:type owl:NamedIndividual ;
         :fromRegion :Valdeorras .


###  http://www.semanticweb.org/alvarobarba/ontologies/2023/3/spanish-wine-ontology#Alella
:Alella rdf:type owl:NamedIndividual ;
        :fromCountry :Espana .


###  http://www.semanticweb.org/alvarobarba/ontologies/2023/3/spanish-wine-ontology#Alta_Alella
:Alta_Alella rdf:type owl:NamedIndividual ;
             :fromRegion :Alella .


###  http://www.semanticweb.org/alvarobarba/ontologies/2023/3/spanish-wine-ontology#Blanc_de_Neu
:Blanc_de_Neu rdf:type owl:NamedIndividual ;
              :isGrapeType :Sparkling ;
              :producedBy :Alta_Alella ;
              :harvestYear "2016"^^xsd:gYear ;
              :hasName "Blanc de Neu" ;
              :hasNumReviews "45"^^xsd:unsignedInt ;
              :hasPrice 16.99 ;
              :hasRating 4.3 ;
              rdfs:comment "Sparkling Wine." .


###  http://www.semanticweb.org/alvarobarba/ontologies/2023/3/spanish-wine-ontology#Bodega_Numanthia
:Bodega_Numanthia rdf:type owl:NamedIndividual ;
                  :fromRegion :Toro .


###  http://www.semanticweb.org/alvarobarba/ontologies/2023/3/spanish-wine-ontology#Cava
:Cava rdf:type owl:NamedIndividual ;
      :hasAcidity 3 ;
      :hasBody 2 .


###  http://www.semanticweb.org/alvarobarba/ontologies/2023/3/spanish-wine-ontology#El_viejo
:El_viejo rdf:type owl:NamedIndividual ;
          :isGrapeType :Toro_Red ;
          :producedBy :Matsu ;
          :harvestYear "2019"^^xsd:gYear ;
          :hasName "El Viejo" ;
          :hasNumReviews "400"^^xsd:unsignedInt ;
          :hasPrice 27.89 ;
          :hasRating 4.2 ;
          rdfs:comment "Normal Red Wine." .


###  http://www.semanticweb.org/alvarobarba/ontologies/2023/3/spanish-wine-ontology#Espana
:Espana rdf:type owl:NamedIndividual ,
                 :Country .


###  http://www.semanticweb.org/alvarobarba/ontologies/2023/3/spanish-wine-ontology#Matsu
:Matsu rdf:type owl:NamedIndividual ;
       :fromRegion :Toro .


###  http://www.semanticweb.org/alvarobarba/ontologies/2023/3/spanish-wine-ontology#Sparkling
:Sparkling rdf:type owl:NamedIndividual ;
           :hasAcidity 3 ;
           :hasBody 2 .


###  http://www.semanticweb.org/alvarobarba/ontologies/2023/3/spanish-wine-ontology#Termanthia
:Termanthia rdf:type owl:NamedIndividual ;
            :isGrapeType :Toro_Red ;
            :producedBy :Bodega_Numanthia ;
            :harvestYear "2004"^^xsd:gYear ;
            :hasName "Termanthia" ;
            :hasNumReviews "28"^^xsd:unsignedInt ;
            :hasPrice 2814.16 ;
            :hasRating 4.6 ;
            rdfs:comment "Expensive Wine." .


###  http://www.semanticweb.org/alvarobarba/ontologies/2023/3/spanish-wine-ontology#Toro
:Toro rdf:type owl:NamedIndividual ;
      :fromCountry :Espana .


###  http://www.semanticweb.org/alvarobarba/ontologies/2023/3/spanish-wine-ontology#Toro_Red
:Toro_Red rdf:type owl:NamedIndividual ;
          :hasAcidity 3 ;
          :hasBody 5 .


###  http://www.semanticweb.org/alvarobarba/ontologies/2023/3/spanish-wine-ontology#Valdeorras
:Valdeorras rdf:type owl:NamedIndividual ;
            :fromCountry :Espana .


###  http://www.semanticweb.org/alvarobarba/ontologies/2023/3/spanish-wine-ontology#200_Cestos_Godello
<http://www.semanticweb.org/alvarobarba/ontologies/2023/3/spanish-wine-ontology#200_Cestos_Godello> rdf:type owl:NamedIndividual ;
                                                                                                    :producedBy :A_Coroa ;
                                                                                                    :harvestYear "2020"^^xsd:gYear ;
                                                                                                    :hasName "200_Cestos_Godello" ;
                                                                                                    :hasNumReviews "33"^^xsd:unsignedInt ;
                                                                                                    :hasPrice 23.7 ;
                                                                                                    :hasRating 4.3 ;
                                                                                                    rdfs:comment "No type Wine." .


#################################################################
#    Annotations
#################################################################

:Cava rdfs:comment "Cava." .


#################################################################
#    General axioms
#################################################################

[ rdf:type owl:AllDisjointClasses ;
  owl:members ( :HighPrice
                :LowPrice
                :MediumPrice
              )
] .


[ rdf:type owl:AllDisjointClasses ;
  owl:members ( :HighRating
                :LowRating
                :MediumRating
              )
] .


[ rdf:type owl:AllDisjointClasses ;
  owl:members ( :Red_Wine
                :Rose_Wine
                :White_Wine
              )
] .


